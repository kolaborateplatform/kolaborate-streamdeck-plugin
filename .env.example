# Environment (development, production, test)
NODE_ENV=development

# Encryption and Security
ENCRYPTION_KEY=                    # Will be auto-generated if not provided
JWT_SECRET=                        # For token-based authentication
ALLOWED_ORIGINS=                   # Comma-separated list of allowed origins

# WebSocket Configuration 
WEBSOCKET_PORT=12345              # Port for WebSocket server
WEBSOCKET_HOST=localhost          # WebSocket host
WEBSOCKET_PATH=/streamdeck        # WebSocket endpoint path

# Stream Deck Configuration
STREAMDECK_PLUGIN_UUID=           # Your plugin's unique identifier
STREAMDECK_PLUGIN_VERSION=1.0.0   # Plugin version
STREAMDECK_PLUGIN_LOGS=           # Path to plugin logs

# Chrome Extension Configuration
CHROME_EXTENSION_ID=              # Your Chrome extension ID
NATIVE_HOST_NAME=                 # Native messaging host name

# Rate Limiting
RATE_LIMIT_WINDOW=60000          # Rate limiting window in milliseconds
RATE_LIMIT_MAX=100              # Maximum requests per window

# Logging
LOG_LEVEL=debug                  # Log level (debug, info, warn, error)
LOG_FILE_PATH=                   # Path to log files