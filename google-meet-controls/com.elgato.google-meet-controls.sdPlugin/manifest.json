{
	"SDKVersion": 2,
	"Author": "Calvin Magezi",
	"CodePath": "bin/plugin.js",
	"PropertyInspectorPath": "propertyinspector/index.html",
	"Description": "Control Google Meet calls from your Stream Deck",
	"Name": "Google Meet Controls",
	"Icon": "images/pluginIcon",
	"URL": "https://www.elgato.com/gaming/stream-deck",
	"Version": "1.0.0",
	"OS": [
		{
			"Platform": "mac",
			"MinimumVersion": "10.11"
		},
		{
			"Platform": "windows",
			"MinimumVersion": "10"
		}
	],
	"Software": {
		"MinimumVersion": "5.0"
	},
	"Nodejs": {
		"Version": "20",
		"Debug": "enabled"
	},
	"UUID": "com.elgato.google-meet-controls",
	"Actions": [
		{
			"Icon": "images/muted",
			"Name": "Mute Toggle",
			"States": [
				{
					"Image": "images/unmuted",
					"TitleAlignment": "middle",
					"FontSize": "16"
				},
				{
					"Image": "images/muted",
					"TitleAlignment": "middle",
					"FontSize": "16"
				}
			],
			"Tooltip": "Toggle microphone mute in Google Meet",
			"UUID": "com.kolaborate.googlemeet.mute"
		},
		{
			"Icon": "images/video",
			"Name": "Video Toggle",
			"States": [
				{
					"Image": "images/video-on",
					"TitleAlignment": "middle",
					"FontSize": "16"
				},
				{
					"Image": "images/video-off",
					"TitleAlignment": "middle",
					"FontSize": "16"
				}
			],
			"Tooltip": "Toggle video in Google Meet",
			"UUID": "com.kolaborate.googlemeet.video"
		},
		{
			"Icon": "images/hand",
			"Name": "Hand Raise",
			"States": [
				{
					"Image": "images/hand-down",
					"TitleAlignment": "middle",
					"FontSize": "16"
				},
				{
					"Image": "images/hand-up",
					"TitleAlignment": "middle",
					"FontSize": "16"
				}
			],
			"Tooltip": "Toggle hand raise in Google Meet",
			"UUID": "com.kolaborate.googlemeet.handRaise"
		},
		{
			"Icon": "images/leave",
			"Name": "Leave Meeting",
			"States": [
				{
					"Image": "images/leave",
					"TitleAlignment": "middle",
					"FontSize": "16"
				}
			],
			"Tooltip": "Leave the current Google Meet call",
			"UUID": "com.kolaborate.googlemeet.leave"
		}
	]
}